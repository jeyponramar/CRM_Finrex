<?xml version="1.0" encoding="utf-8"?>
<setting>
  <module>Menu</module>
  <table>tbl_menu</table>
  <enablepaging>true</enablepaging>
  <pagesize>20</pagesize>
  <toprecords>0</toprecords>
  <enablesorting>true</enablesorting>
  <enablesearch>false</enablesearch>
  <defaultsort>menu_menuid desc</defaultsort>
  <columns>menu_menuname,menu_sequence,menu_url,menu_isvisible</columns>
  <columnlabels>Menu Name,Sequence,URL,Is Visible</columnlabels>
  <gridtype>grid</gridtype>
  <gridcolumn>
    <column>
      <name>menu_menuname</name>
      <headertext>Menu</headertext>
      <row>1</row>
      <width>16%</width>
      <format>
      </format>
    </column>
    <column>
      <name>menu_url</name>
      <headertext>URL</headertext>
      <row>1</row>
      <width>16%</width>
      <format>
      </format>
    </column>
    <column>
      <name>menu_sequence</name>
      <headertext>Sequence</headertext>
      <row>1</row>
      <width>16%</width>
      <format>
      </format>
    </column>
    <column>
      <name>menu_isvisible</name>
      <headertext>Is Visible</headertext>
      <width>25%</width>
      <format>
      </format>
    </column>
  </gridcolumn>
  <hiddencolumns>menu_menuid</hiddencolumns>
  <query>select $COLUMNS$ from tbl_menu $Search$</query>
  <searchbylabels>Menu Name</searchbylabels>
  <searchbycolumns>menu_menuname</searchbycolumns>
  <title>View Menu</title>
  <isrightreport>False</isrightreport>
  <applyviewrights>False</applyviewrights>
</setting>