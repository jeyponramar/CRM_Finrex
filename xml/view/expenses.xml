  <gridtype>grid</gridtype>
  <gridcolumn>
    <column ismobile="true">
      <name>expenses_voucherno</name>
      <headertext>Voucher No</headertext>
      <row>1</row>
      <width>17%</width>
      <format>
      </format>
    </column>
    <column>
      <name>expenses_expensedate</name>
      <headertext>Expense Date</headertext>
