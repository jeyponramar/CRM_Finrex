		</gridcolumn>
		<hiddencolumns>service_serviceid</hiddencolumns>
		<query>select $COLUMNS$ from tbl_service $Search$</query>
		<customequery></customequery>
		<where></where>
		<searchbylabels>Service</searchbylabels>
		<searchbycolumns>service_service</searchbycolumns>
